#include <stdio.h>
 int main() 
{
  int Rows, i, j;
	
  printf(" Enter the Number of Rows:  ");
  scanf("%d", &Rows);
	
  printf("\nPrint Right Angled Triangle \n \n");
  for ( i = 1 ; i <= Rows; i++ ) 
  {
      for ( j = 1 ; j <= i; j++ ) 
      {
          printf(" 01 ");
      }
      printf("\n");
  }
  return 0;
}
